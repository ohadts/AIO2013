	<LINK href="HardwareInfo.css" type="text/css" rel="stylesheet">
<SCRIPT language="VBScript" src="HardwareInfo.vbs">
</SCRIPT>
<SCRIPT language="VBScript">
Sub Window_onLoad
    arrCommands = Split(objTestHTA.commandLine, chr(34))
    For i = 3 to (Ubound(arrCommands) - 1) Step 2
        Document.title = "Hardware Information For " + arrCommands(i)
		SetHostName(arrCommands(i))
    Next
End Sub
</SCRIPT>

